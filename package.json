{
  "name": "aniname",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write . --loglevel=warn",
    "fix": "npm run --silent format; npm run --silent lint -- --fix",
    "gql:generate": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env.local",
    "gql:watch": "npm run gql:generate -- --watch",
    "preinstall": "npx typesync || :",
    "prepare": "npx simple-git-hooks || :"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.1.4",
    "@chakra-ui/react": "^2.7.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.3",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "date-fns": "^2.30.0",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "framer-motion": "^10.12.16",
    "graphql": "^16.7.0",
    "graphql-request": "^6.1.0",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.0",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/import-types-preset": "^2.2.6",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@types/eslint": "8.40.2",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/gtag.js": "^0.0.12",
    "@types/lint-staged": "^13.2.0",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-chakra-ui": "^0.9.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
